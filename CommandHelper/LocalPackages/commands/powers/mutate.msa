mutate:/mutate $vic = >>>

    @plr = player()

    @vic = $vic
    @vic = player(@vic)
    @ploc = ploc()
    @vicLoc = ploc(@vic)

    @x1 = @ploc['x']
    @y1 = @ploc['y']
    @z1 = @ploc['z']

    @x2 = @vicLoc['x']
    @y2 = @vicLoc['y']
    @z2 = @vicLoc['z']

    @x3 = pow(@x1 - @x2, 2)
    @y3 = pow(@y1 - @y2, 2)
    @z3 = pow(@z1 - @z2, 2)

    @final = @x3 + @y3 + @z3

    @final = sqrt(@final)
   
    if(@final >= 10){
        tmsg(@plr,color(red).'Sorry, that player is not close enough.')
        die()
    }

    assign(@cooldown, 60)
    
    assign(@lastUsedMutate, get_value(@plr."lastUsedMutate"))
    if(@lastUsedMutate + @cooldown > time() / 1000,
        assign(@timeleft, subtract(add(@lastUsedMutate, @cooldown), divide(time(),1000)))
        tmsg(@plr,concat('§c[§4', Error, '§c] §7You must wait another §6', round(@timeleft), ' §7seconds before you can use flame again!'))
        die()
    )
    store_value(@plr."lastUsedMutate", time() / 1000)

    runas(~console,'/sudo '.@vic.' c:/mortal')

    include('/home/minecraft/server/plugins/CommandHelper/includes/changeSkin.ms')
    set_timeout(5000,closure(
        _changeSkin(@vic,'','')
    ))

<<<
